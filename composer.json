{
    "name": "raneomik/watchdog-bundle",
    "description": "This bundle provides a configurable watchdog for events to fire",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marek Necesany",
            "email": "marek@choosit.com",
            "homepage": "https://github.com/raneomik"
        }
    ],
    "require": {
        "php": "^7.4|^8.1",
        "symfony/framework-bundle":  "^4.4|^5.4|^6.0",
        "symfony/yaml": "^4.4|^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/test-pack": "*",
        "symfony/debug-pack": "*",
        "vimeo/psalm": "^4.22",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "symplify/coding-standard": "^10.1",
        "psalm/plugin-symfony": "^3.1",
        "infection/infection": "^0.26.5",
        "symplify/phpstan-rules": "^10.1"
    },
    "autoload": {
        "psr-4": { "Raneomik\\WatchdogBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Raneomik\\WatchdogBundle\\Test\\": "tests/" }
    },
    "scripts": {
        "test": [
            "vendor/bin/simple-phpunit"
        ]
    }
}
